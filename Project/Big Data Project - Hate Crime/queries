offence_by_offence_type - SELECT OFFCOD1EXTNEW,STATECOD,COUNT(OFFCOD1) FROM data GROUP BY OFFCOD1EXTNEW,STATECOD

offence_by_bias - SELECT OFFCOD1EXTNEW,BIASMO1EXTNEW,COUNT(OFFCOD1) FROM data GROUP BY OFFCOD1EXTNEW,BIASMO1EXTNEW

offence_by_bias_by_state - SELECT OFFCOD1EXTNEW,BIASMO1EXTNEW,STATECOD,COUNT(OFFCOD1) FROM data GROUP BY OFFCOD1EXTNEW,BIASMO1EXTNEW,STATECOD

offence_by_location - SELECT OFFCOD1EXTNEW,LOCCOD1EXTNEW,COUNT(OFFCOD1) FROM data GROUP BY OFFCOD1EXTNEW,LOCCOD1EXTNEW

offence_by_location_by_state-SELECT OFFCOD1EXTNEW,LOCCOD1EXTNEW,STATECOD,COUNT(OFFCOD1) FROM data GROUP BY OFFCOD1EXTNEW,LOCCOD1EXTNEW,STATECOD

offence_by_offenders_race-SELECT STATECOD,GOFFRACEXTNEW,COUNT(OFFCOD1) FROM data GROUP BY STATECOD,GOFFRACEXTNEW

offence_by_victim_by_state - SELECT BIASMO1EXTNEW,STATECOD,COUNT(OFFCOD1) FROM data GROUP BY BIASMO1EXTNEW,STATECOD

Heatmap - SELECT BIASMO1EXTNEW,GOFFRACEXTNEW,COUNT(OFFCOD1) FROM data GROUP BY BIASMO1EXTNEW,GOFFRACEXTNEW

YY_MM_Offence_type - SELECT MASTERYR,MONTH,OFFCOD1EXTNEW,COUNT(OFFCOD1) FROM data GROUP BY MASTERYR,MONTH,OFFCOD1EXTNEW

YY_MM_offender - SELECT MASTERYR,MONTH,GOFFRACEXTNEW,COUNT(OFFCOD1) FROM data GROUP BY MASTERYR,MONTH,GOFFRACEXTNEW

YY_MM_victim - SELECT MASTERYR,MONTH,BIASMO1EXTNEW,COUNT(OFFCOD1) FROM data GROUP BY MASTERYR,MONTH,BIASMO1EXTNEW
